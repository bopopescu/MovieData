    	<script>
    	var ThousandBestData = {
	labels : ["30","40","50","60","70","80","90","00","10","20"],
	datasets : [
{//yes
            fillColor : "rgba(220,220,220,0.5)",
            strokeColor : "rgba(220,220,220,1)",
            data : [65,59,90,81,56,55,40]
        },
        {//no
            fillColor : "rgba(151,187,205,0.5)",
            strokeColor : "rgba(151,187,205,1)",
            data : [28,48,40,19,96,27,100]
        }
	]
}
    var ThousandBest = document.getElementById('ThousandBest').getContext('2d');
    new Chart(ThousandBest).Bar(ThousandBestData);

		</script>
